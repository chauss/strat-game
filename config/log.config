[loggers]
keys=root,model

[handlers]
keys=consoleHandler,fileHandler

[formatters]
keys=std

[logger_root]
level=DEBUG
handlers=consoleHandler

[logger_model]
level=DEBUG
handlers=consoleHandler,fileHandler
qualname=model
propagate=0

[handler_consoleHandler]
class=StreamHandler
level=DEBUG
formatter=std
args=(sys.stdout,)

[handler_fileHandler]
class=handlers.RotatingFileHandler
level=DEBUG
formatter=std
args=('StratGame.log', 'a', 10485760)

[formatter_std]
format=%(asctime)s [%(module)-10s: %(levelname)-8s] %(message)s
datefmt=%Y/%m/%d %H:%M:%S